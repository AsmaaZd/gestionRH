<section class="ftco-section">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-md-6 text-center mb-5">
					<h2 class="heading-section">Calendar #09</h2>
				</div>
			</div>
			<div class="row justify-content-center">
				<div class="col-md-4 text-center">
					<h3 class="h5">Calendar Datepicker</h3>
					<div class="w-100">
            <form action="" method="post" class="datepickers">
              <div class="form-group">
                <!-- <label class="label-control" for="id_start_datetime">Datetime picker</label> -->
                <div class="input-group date" id="id_0">
                  <input type="text" value="" class="form-control" placeholder="MM/DD/YYYY hh:mm:ss" required/>
                </div>
              </div>
            </form>
					</div>
				</div>
			</div>
		</div>
	</section>
{{ form_start(form) }}
<div class="modal-body">
<div class="mb-3">
	{{ form_widget(form.title) }}
</div>

<div class="mb-3 ">
	{{ form_label(form.start, 'Date debut', {'label_attr': {'class': 'visually-hidden'}}) }}
	<div class="input-group">
		<div class="input-group-text">
			<i class="fa fa-calendar"></i>
		</div>
		<div class="form-control">{{ form_widget(form.start) }}</div>
	</div>
</div>




<div class="mb-3">
	{{ form_label(form.end, 'Date fin', {'label_attr': {'class': 'visually-hidden'}}) }}
	<div class="input-group">
		<div class="input-group-text">
			<i class="fa fa-calendar"></i>
		</div>
		{{ form_widget(form.end) }}
	</div>
</div>
<div class="mb-3">
	{{ form_widget(form.description) }}
</div>
<div class="mb-3">
	<div class="form-check">
		{{ form_widget(form.all_day) }}
		{{ form_label(form.all_day, 'Toute la journ√©e?', {'label_attr': {'class': 'form-check-label'}}) }}
	</div>
</div>


<div class="mb-3 row">
	<div class="col">
		<div>
			{# {{ form_label(form.background_color, 'Arriere plan', {'label_attr': {'class': 'visually-hidden'}}) }} #}
			<div class="input-group d-flex align-items-center">
				<div class="input-group-text mx-2">
					<i class="fas fa-palette"></i>
				</div>
				{{ form_widget(form.background_color) }}
			</div>
		</div>
	</div>
	<div class="col">
		<div>
			<div class="input-group d-flex align-items-center ">
				<div class="input-group-text mx-2">
					<i class="fas fa-pencil-alt"></i>
				</div>
				{{ form_widget(form.text_color) }}
			</div>
		</div>
	</div>
	<div class="col">
		<div>
			{# {{ form_label(form.background_color, 'Arriere plan', {'label_attr': {'class': 'visually-hidden'}}) }} #}
			<div class="input-group d-flex align-items-center">
				<div class="input-group-text mx-2">
					<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-app" viewbox="0 0 16 16">
						<path d="M11 2a3 3 0 0 1 3 3v6a3 3 0 0 1-3 3H5a3 3 0 0 1-3-3V5a3 3 0 0 1 3-3h6zM5 1a4 4 0 0 0-4 4v6a4 4 0 0 0 4 4h6a4 4 0 0 0 4-4V5a4 4 0 0 0-4-4H5z"/>
					</svg>
				</div>
				{{ form_widget(form.border_color) }}
			</div>
		</div>
	</div>
</div>
</div>
<div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Annuler</button>
        <button type="submit" class="btn btn-primary">{{ button_label|default('Upload') }}</button>
        
      </div>
{{ form_end(form) }}
